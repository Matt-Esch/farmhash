{
  "name": "farmhash",
  "version": "3.0.0-rc1",
  "author": "Lovell Fuller <npm@lovell.info>",
  "contributors": [
    "Matt Ranney  <mjr@ranney.com>",
    "Jonas Hermsmeier <jhermsmeier@gmail.com> (https://jhermsmeier.de)"
  ],
  "description": "Node.js implementation of FarmHash, Google's family of very fast hash functions",
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "test": "semistandard && cc && node test/unit && prebuild-ci"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/lovell/farmhash"
  },
  "keywords": [
    "farmhash",
    "hash",
    "cityhash",
    "murmurhash",
    "fingerprint"
  ],
  "dependencies": {
    "node-addon-api": "^1.6.3",
    "prebuild-install": "^5.3.0"
  },
  "devDependencies": {
    "cc": "^1.0.2",
    "prebuild": "^9.0.0",
    "prebuild-ci": "^3.1.0",
    "semistandard": "^13.0.1"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.10.0"
  },
  "binary": {
    "napi_versions": [
      2,
      3,
      4
    ]
  },
  "cc": {
    "linelength": "120",
    "ignore": [
      "node_modules",
      "src/upstream"
    ]
  }
}
